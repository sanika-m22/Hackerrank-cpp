#include <iostream>
#include <string>
#include <sstream>

struct Student {
    int age;
    std::string first_name;
    std::string last_name;
    int standard;
};

int main() {
    Student st;

    std::cin >> st.age;
    
    std::cin >> st.first_name;
    
    std::cin >> st.last_name;
    
    std::cin >> st.standard;

    std::cout << st.age << " " 
              << st.first_name << " " 
              << st.last_name << " " 
              << st.standard << std::endl;

    return 0;
}
